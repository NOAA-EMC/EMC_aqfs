#!/bin/ksh
######################################################################
#  UNIX Script Documentation Block
#                      .
# Script name:         exaqm_cmaq_maxi.sh
# Script description:  CMAQ post processing for daily surface maximum O3/PM2.5
#
# Youhua Tang  Org: NP22  Date: 2009-06-30
# Jianping Huang : 10/1/2016
#          Change grib1 to grib2 
#          added new products for PM2.5 with WMO Headers  
#
######################################################################
set -xa

export DBNALERT_TYPE=${DBNALERT_TYPE:-GRIB_HIGH}

cd $DATA

while [ ! -s ${COMIN}/aqm.${cycle}.aconc_sfc.ncf ]; do
 sleep 120    # wait until the CMAQ output is ready
done

export CMAQFILE1=${COMIN}/aqm.${cycle}.aconc_sfc.ncf
if [ -s ${COMIN}/aqm.t00z.aconc_sfc.ncf ] ; then 
 export CMAQFILE2=${COMIN}/aqm.t00z.aconc_sfc.ncf
elif [ -s ${COMINm1}/aqm.t12z.aconc_sfc.ncf ];  then 
 export CMAQFILE2=${COMINm1}/aqm.t12z.aconc_sfc.ncf
elif [ -s ${COMINm2}/aqm.t12z.aconc_sfc.ncf ];  then
 export CMAQFILE2=${COMINm2}/aqm.t12z.aconc_sfc.ncf
fi

if [ $cyc -eq 06 ]; then
 if [ -s ${COMIN}/aqm.t00z.aconc_sfc.ncf ] ; then
  export CMAQFILE3=${COMIN}/aqm.t00z.aconc_sfc.ncf
 elif [ -s ${COMINm1}/aqm.t12z.aconc_sfc.ncf ] ; then
  export CMAQFILE3=${COMINm1}/aqm.t12z.aconc_sfc.ncf
 elif [ -s ${COMINm2}/aqm.t12z.aconc_sfc.ncf ] ; then
  export CMAQFILE3=${COMINm2}/aqm.t12z.aconc_sfc.ncf
 fi
fi
if [ $cyc -eq 12 ]; then
 if [ -s ${COMIN}/aqm.t06z.aconc_sfc.ncf ] ; then 
  export CMAQFILE3=${COMIN}/aqm.t06z.aconc_sfc.ncf
 elif [ -s ${COMINm1}/aqm.t12z.aconc_sfc.ncf ] ; then
  export CMAQFILE3=${COMINm1}/aqm.t12z.aconc_sfc.ncf
 elif [ -s ${COMINm2}/aqm.t12z.aconc_sfc.ncf ] ; then
  export CMAQFILE3=${COMINm2}/aqm.t12z.aconc_sfc.ncf
 fi
fi

cat >cmaq-maxi2grib.ini <<EOF5
&control
markutc=05
outfile='aqm-maxi.139.grib2'
varlist='o3_1hr','o3_8hr'
id_gribdomain=139
/
EOF5

startmsg
$EXECaqm/aqm_post_maxi_CHA_grib2_v2   >> ${pgmout} 2>errfile
export err=$?;err_chk

# interpolate to grid 196


$WGRIB2 aqm-maxi.139.grib2 |grep "OZMAX1" | $WGRIB2 -i aqm-maxi.139.grib2 -grib  aqm.${cycle}.1ho3-max.139.grib2
$WGRIB2 aqm-maxi.139.grib2 |grep "OZMAX8" | $WGRIB2 -i aqm-maxi.139.grib2 -grib  aqm.${cycle}.8ho3-max.139.grib2

export grid196="10 6 0 0 0 0 0 0 321 225 18073000 198475000 56 20000000 23088000 206131000 64 0 2500000 2500000"
$COPYGB2  -g "$grid196" -x  -i"1 1"  aqm-maxi.139.grib2  aqm-maxi.196.grib2 
$WGRIB2 aqm-maxi.196.grib2 |grep "OZMAX1" | $WGRIB2 -i aqm-maxi.196.grib2 -grib  aqm-1hro3-maxi.196.grib2
$WGRIB2 aqm-maxi.196.grib2 |grep "OZMAX8" | $WGRIB2 -i aqm-maxi.196.grib2 -grib  aqm-8hro3-maxi.196.grib2

# write out pm2.5 in grib2 format

cat >cmaq-maxi2grib.ini <<EOF5
&control
markutc=05
outfile='aqm-pm25_24hr.139.grib2'
varlist='pm25_1hr','pm25_24hr'
id_gribdomain=139
/
EOF5

startmsg
$EXECaqm/aqm_post_maxi_CHA_grib2_v2  >> ${pgmout} 2>errfile
export err=$?;err_chk

$COPYGB2  -g "$grid196" -x  -i"1 1" aqm-pm25_24hr.139.grib2 aqm-pm25_24hr.196.grib2

#-----------------------------------------
$WGRIB2 aqm-pm25_24hr.139.grib2 |grep "PDMAX1" | $WGRIB2 -i aqm-pm25_24hr.139.grib2 -grib aqm.${cycle}.1hpm25-max.139.grib2
export err=$?;err_chk

$WGRIB2 aqm-pm25_24hr.139.grib2 |grep "PMTF" | $WGRIB2 -i aqm-pm25_24hr.139.grib2 -grib  aqm.${cycle}.24hpm25-ave.139.grib2
export err=$?;err_chk
#-----------------------------------------
$WGRIB2 aqm-pm25_24hr.196.grib2 |grep "PDMAX1" | $WGRIB2 -i aqm-pm25_24hr.196.grib2 -grib aqm.${cycle}.1hpm25-max.196.grib2
export err=$?;err_chk
$WGRIB2 aqm-pm25_24hr.196.grib2 |grep "PMTF"   | $WGRIB2 -i aqm-pm25_24hr.196.grib2 -grib aqm.${cycle}.24hrpm25-ave.196.grib2
export err=$?;err_chk

if [ "1" = "2" ] ; then
 
for hr in 1 8
do

#$CNVGRIB -g12 -m -p32  aqm-${hr}hro3-maxi.196.grib aqm-${hr}hro3-maxi.196.grib2
 
echo 0 > filesize
export XLFRTEOPTS="unit_vars=yes"
export FORT11=aqm-${hr}hro3-maxi.196.grib2
export FORT12="filesize"
export FORT31=
export FORT51=aqm-${hr}hro3-maxi.196.grib2.temp
${TOCGRIB2SUPER} < $PARMaqm/wmo/grib2_cmaq-${hr}hro3-maxi.${cycle}.196

echo `ls -l  aqm-${hr}hro3-maxi.196.grib2.temp | awk '{print $5} '` > filesize
export XLFRTEOPTS="unit_vars=yes"
export FORT11=aqm-${hr}hro3-maxi.196.grib2.temp
export FORT12="filesize"
export FORT31=
#export FORT51=grib2.${envir}.${cycle}.aqm-${hr}hro3-max.196
export FORT51=awpaqm.${cycle}.${hr}ho3-max.196.grib2
${TOCGRIB2SUPER} < $PARMaqm/wmo/grib2_cmaq-${hr}hro3-maxi.${cycle}.196

done 
########################################################
# Add WMO header for daily 1h PM2.5 and 24hr_ave PM2.5
########################################################
#  daily_1hr_max_PM2.5
rm -rf filesize
echo 0 > filesize
export XLFRTEOPTS="unit_vars=yes"
export FORT11=aqm.${cycle}.1hpm25-max.196.grib2
export FORT12="filesize"
export FORT31=
export FORT51=aqm.${cycle}.max_1hr_pm25.196.grib2.temp
${TOCGRIB2SUPER} < $PARMaqm/wmo/grib2_aqm_max_1hr_pm25.${cycle}.196

echo `ls -l  aqm.${cycle}.max_1hr_pm25.196.grib2.temp | awk '{print $5} '` > filesize
export XLFRTEOPTS="unit_vars=yes"
export FORT11=aqm.${cycle}.max_1hr_pm25.196.grib2.temp
export FORT12="filesize"
export FORT31=
export FORT51=awpaqm.${cycle}.daily-1hr-pm25-max.196.grib2
${TOCGRIB2SUPER} < $PARMaqm/wmo/grib2_aqm_max_1hr_pm25.${cycle}.196


rm filesize
#  daily_24hr_ave_PM2.5
echo 0 > filesize
export XLFRTEOPTS="unit_vars=yes"
export FORT11=aqm.${cycle}.24hrpm25-ave.196.grib2
export FORT12="filesize"
export FORT31=
export FORT51=aqm.${cycle}.24hrpm25-ave.196.grib2.temp
${TOCGRIB2SUPER} < $PARMaqm/wmo/grib2_aqm_ave_24hrpm25_awp.${cycle}.196

#echo `ls -l  aqm.${cycle}.24hrpm25-ave.196.grib2 | awk '{print $5} '` > filesize
echo `ls -l  aqm.${cycle}.24hrpm25-ave.196.grib2.temp | awk '{print $5} '` > filesize
export XLFRTEOPTS="unit_vars=yes"
export FORT11=aqm.${cycle}.24hrpm25-ave.196.grib2.temp
export FORT12="filesize"
export FORT31=
export FORT51=awpaqm.${cycle}.24hr-pm25-ave.196.grib2
${TOCGRIB2SUPER} < $PARMaqm/wmo/grib2_aqm_ave_24hrpm25_awp.${cycle}.196


       ##############################
       # Post Files to PCOM
       ##############################

       if test "$SENDCOM" = 'YES'
       then
           cp awpaqm.${cycle}.*o3-max.196.grib2            $PCOM/
           cp awpaqm.${cycle}.daily-1hr-pm25-max.196.grib2 $PCOM/ 
           cp awpaqm.${cycle}.24hr-pm25-ave.196.grib2      $PCOM/ 

          ##############################
          # Distribute Data
          ##############################

          if [ "$SENDDBN_NTC" = 'YES' ] ; then
            $DBNROOT/bin/dbn_alert $DBNALERT_TYPE $NET $job $PCOM/awpaqm.${cycle}.1ho3-max.196.grib2
            $DBNROOT/bin/dbn_alert $DBNALERT_TYPE $NET $job $PCOM/awpaqm.${cycle}.8ho3-max.196.grib2
            $DBNROOT/bin/dbn_alert $DBNALERT_TYPE $NET $job $PCOM/awpaqm.${cycle}.daily-1hr-pm25-max.196.grib2
            $DBNROOT/bin/dbn_alert $DBNALERT_TYPE $NET $job $PCOM/awpaqm.${cycle}.24hr-pm25-ave.196.grib2
          else
             msg="File $output_grb.$job not posted to db_net."
             postmsg "$jlogfile" "$msg"
          fi
       fi

fi
#done

if [ $cyc -eq 06 -o $cyc -eq 12 ] && [ "${SENDCOM}" = "YES" ]; then
  cp aqm-1hro3-maxi.196.grib2             $COMOUT/aqm.${cycle}.max_1hr_o3.196.grib2
  cp aqm-8hro3-maxi.196.grib2             $COMOUT/aqm.${cycle}.max_8hr_o3.196.grib2
  cp aqm.${cycle}.1ho3-max.139.grib2      $COMOUT/aqm.${cycle}.max_1hr_o3.139.grib2
  cp aqm.${cycle}.8ho3-max.139.grib2      $COMOUT/aqm.${cycle}.max_8hr_o3.139.grib2
  cp aqm.${cycle}.1hpm25-max.196.grib2    $COMOUT/aqm.${cycle}.max_1hr_pm25.196.grib2
  cp aqm.${cycle}.24hrpm25-ave.196.grib2  $COMOUT/aqm.${cycle}.ave_24hr_pm25.196.grib2
  cp aqm.${cycle}.1hpm25-max.139.grib2    $COMOUT/aqm.${cycle}.max_1hr_pm25.139.grib2
  cp aqm.${cycle}.24hpm25-ave.139.grib2   $COMOUT/aqm.${cycle}.ave_24hr_pm25.139.grib2
 if [ "$SENDDBN" = 'YES' ] ; then
  $DBNROOT/bin/dbn_alert MODEL AQM_MAX $job $COMOUT/aqm.${cycle}.max_1hr_o3.196.grib2
  $DBNROOT/bin/dbn_alert MODEL AQM_MAX $job $COMOUT/aqm.${cycle}.max_8hr_o3.196.grib2
  $DBNROOT/bin/dbn_alert MODEL AQM_MAX $job $COMOUT/aqm.${cycle}.max_1hr_pm25.196.grib2
  $DBNROOT/bin/dbn_alert MODEL AQM_MAX $job $COMOUT/aqm.${cycle}.ave_24hr_pm25.196.grib2
 fi
fi

